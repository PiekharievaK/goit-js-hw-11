
<div class="img-card">
    <a class="gallery__link" href={{largeImageURL}}>
        <img src="{{webformatURL}}" alt="{{tags}}" loading="lazy" width="50px"/>
    </a>
      <img
      src="./partials/demo.jpg"
      alt="{{original_title}}"
      class="movie-card__image"
      width="100px"
    />
    <div class="info">
        <p class="info_item">
            <b>Likes </b><br>{{likes}}
        </p>
        <p class="info_item">
            <b>Views</b> <br>{{views}}
        </p>
        <p class="info_item">
            <b>Comments</b> <br>{{comments}}
        </p>
        <p class="info_item">
            <b>Downloads</b> <br>{{downloads}}
        </p>
    </div>
</div>
